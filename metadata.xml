			<lom:langstring>...</lom:langstring>
		</lom:title>
		<!-- Entrada de catálogo (0..*) -->
		<lom:catalogentry>
			<!-- Nombre de catálogo -->
			<lom:catalog>...</lom:catalog>
			<!-- Nombre de entrada de catálogo -->
			<lom:entry>
				<lom:langstring>...</lom:langstring>
			</lom:entry>
		</lom:catalogentry>
		<!-- Idioma del objeto (0..*) -->
