			</lom:centity>
			<!-- Fecha de contribución -->
			<lom:date>
				<lim:datetime>...</lim:datetime>
			</lom:date>
		</lom:contribute>
	</lom:lifecycle>
	<!-- Categoría meta-metadatos -->
