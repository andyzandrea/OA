		<!-- Palabras clave (0..*) -->
		<lom:keyword>
			<lom:langstring>...</lom:langstring>
		</lom:keyword>
		<!-- Cobertura del objeto (0..*) -->
		<lom:coverage>
			<lom:langstring>...</lom:langstring>
		</lom:coverage>
		<!-- Estructura (0..1) -->
		<lom:structure>
			<lom:source>
				<lom:langstring>LOMv1.0</lom:langstring>
